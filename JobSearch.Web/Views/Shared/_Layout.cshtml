<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @*<title>@ViewBag.Title</title>*@
    <title>Job Search Portal</title>
    @*@Styles.Render("~/Content/css")*@
    @Styles.Render("~/Content/assets/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400italic,400,600,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,300italic,400italic,700,400,300' rel='stylesheet' type='text/css'>
    <!-- FAVICONS -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/assets/ico/repute144x144.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/assets/ico/repute114x114.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/assets/ico/repute72x72.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/assets/ico/repute57x57.png">
    <link rel="shortcut icon" href="~/Content/assets/ico/favicon.png">
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/MyScripts/JobsController.js"></script>
</head>
<body>

    <!-- WRAPPER -->
    <div class="wrapper">

        <!-- PAGE CONTENT -->
        @RenderPage("~/Views/Partial/Header.cshtml");


        @RenderBody()

        <!-- END PAGE CONTENT -->
        @RenderPage("~/Views/Partial/Footer.cshtml");
    </div>

    <!-- END WRAPPER -->
    <!-- JAVASCRIPTS -->
    <script src="~/content/assets/js/jquery-2.1.1.min.js"></script>
    <script src="~/content/assets/js/bootstrap.min.js"></script>
    <script src="~/content/assets/js/plugins/autohidingnavbar/jquery.bootstrap-autohidingnavbar.min.js"></script>
    <script src="~/content/assets/js/repute-scripts.js"></script>
    <script src="~/content/assets/js/plugins/slick/slick.min.js"></script>
    <script src="~/content/assets/js/plugins/stellar/jquery.stellar.min.js"></script>
    <script src="~/content/assets/js/plugins/jquery-easypiechart/jquery.easypiechart.min.js"></script>
    <script src="~/Content/shop/assets/js/repute-shop.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
